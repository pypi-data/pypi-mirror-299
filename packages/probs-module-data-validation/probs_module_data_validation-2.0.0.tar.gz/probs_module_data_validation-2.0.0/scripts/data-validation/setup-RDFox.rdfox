
# Create datastore
dstore create default type parallel-nn

# Printing on stdout by default
set output out

# Set directories
set dir.facts "$(dir.root)/data"
set dir.output "$(dir.root)/data"
set dir.scripts "$(dir.root)/scripts/data-validation"
set dir.dlog  "$(dir.root)/scripts/data-validation"

# Load prefixes
exec set_prefixes
