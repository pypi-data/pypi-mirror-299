<odoo>

    <record id="general_audit_ws_c0d0898_view_search" model="ir.ui.view">
        <field name="name">general_audit_ws_c0d0898 search</field>
        <field name="model">general_audit_ws_c0d0898</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_search"
        />
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <data />
        </field>
    </record>

    <record id="general_audit_ws_c0d0898_view_tree" model="ir.ui.view">
        <field name="name">general_audit_ws_c0d0898 tree</field>
        <field name="model">general_audit_ws_c0d0898</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_tree"
        />
        <field name="arch" type="xml">
            <data>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_c0d0898_view_form" model="ir.ui.view">
        <field name="name">general_audit_ws_c0d0898 form</field>
        <field name="model">general_audit_ws_c0d0898</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_form"
        />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//page[1]" position="before">
                    <page name="detail" string="Details">

                        <field name="detail_ids">
                            <tree editable="top" create="0" delete="0">
                                <field
                                    name="going_concern_category_id"
                                    string="Category"
                                />
                                <field name="going_concern_id" string="Item" />
                                <field
                                    name="going_concern_exist"
                                    string="Exist"
                                    widget="boolean_toggle"
                                />
                                <field name="consideration" />
                            </tree>
                        </field>
                    </page>
                </xpath>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_c0d0898_action" model="ir.actions.act_window">
        <field name="name">Going Concern Analysis</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">general_audit_ws_c0d0898</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="general_audit_ws_c0d0898_menu"
        name="Going Concern Analysis"
        parent="ssi_general_audit.menu_general_audit_risk_assesment"
        action="general_audit_ws_c0d0898_action"
        sequence="1503"
        groups="general_audit_ws_c0d0898_viewer_group"
    />


</odoo>
