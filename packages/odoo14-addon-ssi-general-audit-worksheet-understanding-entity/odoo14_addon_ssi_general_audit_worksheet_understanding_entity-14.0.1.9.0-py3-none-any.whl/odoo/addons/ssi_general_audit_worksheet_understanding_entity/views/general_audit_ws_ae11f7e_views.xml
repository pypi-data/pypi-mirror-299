<odoo>

    <record id="general_audit_ws_ae11f7e_view_search" model="ir.ui.view">
        <field name="name">general_audit_ws_ae11f7e form</field>
        <field name="model">general_audit_ws_ae11f7e</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_search"
        />
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <data />
        </field>
    </record>

    <record id="general_audit_ws_ae11f7e_view_tree" model="ir.ui.view">
        <field name="name">general_audit_ws_ae11f7e tree</field>
        <field name="model">general_audit_ws_ae11f7e</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_tree"
        />
        <field name="arch" type="xml">
            <data>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_ae11f7e_view_form" model="ir.ui.view">
        <field name="name">general_audit_ws_ae11f7e form</field>
        <field name="model">general_audit_ws_ae11f7e</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_form"
        />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//page[1]" position="before">
                    <page name="business_cycle" string="Business Cycles">
                        <field name="business_cycle_ids" />
                    </page>
                    <page name="exprt" string="Experts">
                        <field name="expert_ids">
                            <tree>
                                <field name="name" />
                                <field name="type_id" />
                                <field name="documentation" />
                                <field
                                    name="related_account_type_ids"
                                    widget="many2many_tags"
                                />
                            </tree>
                            <form>
                                <group name="expert_1" colspan="4" col="2">
                                    <field name="name" />
                                    <field name="type_id" />
                                    <field name="documentation" />
                                    <field
                                        name="related_account_type_ids"
                                        widget="many2many_tags"
                                    />
                                </group>
                            </form>
                        </field>
                    </page>
                    <page
                        name="significant_information"
                        string="Significant Informations"
                    >
                        <separator string="Previous Audit" />
                        <field name="previous_audit_information_ids">
                            <tree>
                                <field name="name" />
                                <field name="documentation" />
                                <field
                                    name="related_account_type_ids"
                                    widget="many2many_tags"
                                />
                            </tree>
                            <form>
                                <group
                                    name="previous_audit_information_1"
                                    colspan="4"
                                    col="2"
                                >
                                    <field name="name" />
                                    <field name="documentation" />
                                    <field
                                        name="related_account_type_ids"
                                        widget="many2many_tags"
                                    />
                                </group>
                            </form>
                        </field>
                        <separator string="Previous Other" />
                        <field name="previous_other_information_ids">
                            <tree>
                                <field name="name" />
                                <field name="documentation" />
                                <field
                                    name="related_account_type_ids"
                                    widget="many2many_tags"
                                />
                            </tree>
                            <form>
                                <group
                                    name="previous_other_information_1"
                                    colspan="4"
                                    col="2"
                                >
                                    <field name="name" />
                                    <field name="documentation" />
                                    <field
                                        name="related_account_type_ids"
                                        widget="many2many_tags"
                                    />
                                </group>
                            </form>
                        </field>
                        <separator string="Other" />
                        <field name="other_information_ids">
                            <tree>
                                <field name="name" />
                                <field name="documentation" />
                                <field
                                    name="related_account_type_ids"
                                    widget="many2many_tags"
                                />
                            </tree>
                            <form>
                                <group name="other_information_1" colspan="4" col="2">
                                    <field name="name" />
                                    <field name="documentation" />
                                    <field
                                        name="related_account_type_ids"
                                        widget="many2many_tags"
                                    />
                                </group>
                            </form>
                        </field>
                    </page>
                    <page name="other_report" string="Other Reports">
                        <group name="other_report" colspan="4" col="2">
                            <field
                                name="other_report_ids"
                                widget="many2many_checkboxes"
                            />
                        </group>
                    </page>
                </xpath>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_ae11f7e_action" model="ir.actions.act_window">
        <field name="name">Main Business Activity Process</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">general_audit_ws_ae11f7e</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="general_audit_ws_ae11f7e_menu"
        name="Main Business Activity Processes"
        parent="ssi_general_audit.menu_general_audit_risk_assesment"
        action="general_audit_ws_ae11f7e_action"
        sequence="1503"
        groups="general_audit_ws_ae11f7e_viewer_group"
    />


</odoo>
