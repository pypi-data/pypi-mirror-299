<odoo>

<record id="general_audit_standard_detail_view_form" model="ir.ui.view">
    <field name="name">general_audit.standard_detail - form</field>
    <field name="model">general_audit.standard_detail</field>
    <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_standard_detail_view_form"
        />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//notebook" position="inside">
                <page name="understanding_entity" string="Understanding Entity">
                    <group
                            name="understanding_entity_1"
                            colspan="4"
                            col="1"
                            string="Impacted By"
                        >
                        <field name="a13a30e_detail_ids" invisible="1" />
                        <field name="regulation_impacted" string="Regulation" />
                        <field name="bdcdfc5_detail_ids" invisible="1" />
                        <field
                                name="business_environmeny_impacted"
                                string="Business Environment"
                            />
                        <field name="c0e0eec_detail_ids" invisible="1" />
                        <field name="fraud_impacted" string="Fraud" />
                        <field name="expert_impacted" string="Uses of Expert" />
                        <field
                                name="previous_audit_information_impacted"
                                string="Previous Significant Audit Information"
                            />
                        <field
                                name="previous_other_information_impacted"
                                string="Previous Significant Other Information"
                            />
                        <field
                                name="other_information_impacted"
                                string="Other Significant Information"
                            />
                    </group>
                </page>
            </xpath>
        </data>
    </field>
</record>
</odoo>
