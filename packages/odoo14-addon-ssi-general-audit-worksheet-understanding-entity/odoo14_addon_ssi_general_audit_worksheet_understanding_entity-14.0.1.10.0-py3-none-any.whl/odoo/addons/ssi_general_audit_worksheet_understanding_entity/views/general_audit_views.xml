<odoo>

<record id="general_audit_view_form" model="ir.ui.view">
    <field name="name">general_audit -  form</field>
    <field name="model">general_audit</field>
    <field name="mode">extension</field>
    <field name="inherit_id" ref="ssi_general_audit.general_audit_view_form" />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//page[last()]" position="before">
                <page name="understanding_entity" string="Understanding Entity">
                    <group name="understanding_entity_1" colspan="4" col="2">
                        <field name="business_cycle_ids" widget="many2many_tags" />
                        <field name="other_report_ids" widget="many2many_tags" />
                        <field name="expert_type_ids" widget="many2many_tags" />
                    </group>
                </page>
            </xpath>
        </data>
    </field>
</record>

</odoo>
