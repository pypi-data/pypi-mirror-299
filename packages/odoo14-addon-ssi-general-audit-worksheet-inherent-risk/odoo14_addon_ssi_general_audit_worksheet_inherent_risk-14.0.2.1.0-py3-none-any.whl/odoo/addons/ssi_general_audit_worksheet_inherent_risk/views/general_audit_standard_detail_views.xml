<odoo>

<record id="general_audit_standard_detail_view_form" model="ir.ui.view">
    <field name="name">general_audit.standard_detail - form</field>
    <field name="model">general_audit.standard_detail</field>
    <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_standard_detail_view_form"
        />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//notebook" position="inside">
                <page name="inherent_risk" string="Inherent Risk">
                    <group name="inherent_risk_1" colspan="4" col="2">
                        <field name="inherent_risk" />
                        <field name="significant_risk" />
                    </group>
                </page>
            </xpath>
        </data>
    </field>
</record>
</odoo>
