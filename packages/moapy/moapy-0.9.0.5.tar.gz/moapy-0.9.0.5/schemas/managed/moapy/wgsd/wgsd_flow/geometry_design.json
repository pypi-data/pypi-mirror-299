{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/python-executor/"
    },
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/execute?functionId=moapy/wgsd/wgsd_flow/geometry_design": {
      "post": {
        "summary": "Geometry Design",
        "description": "Return the geometry\n\nArgs:\n    concrete (ConcreteGeometry): concrete geometry\n    rebar (RebarGeometry): rebar geometry\n\nReturns:\n    Geometry: geometry",
        "operationId": "geometry design",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/geometry_design"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Geometry"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ConcreteGeometry": {
        "properties": {
          "material": {
            "description": "Material of the concrete",
            "default": {
              "design_code": "ACI318M-19",
              "grade": "C12"
            },
            "$ref": "#/components/schemas/ConcreteGrade"
          },
          "outerPolygon": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Outerpolygon",
            "description": "Outer polygon of the concrete",
            "default": [
              {
                "x": 0.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 600.0
              },
              {
                "x": 0.0,
                "y": 600.0
              }
            ]
          },
          "innerPolygon": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Innerpolygon",
            "description": "Inner polygon of the concrete",
            "default": [
              {
                "x": 80.0,
                "y": 80.0
              },
              {
                "x": 320.0,
                "y": 80.0
              },
              {
                "x": 320.0,
                "y": 520.0
              },
              {
                "x": 80.0,
                "y": 520.0
              }
            ]
          }
        },
        "type": "object",
        "title": "GSD Concrete Geometry",
        "dataclassname": "moapy.rc_pre.ConcreteGeometry"
      },
      "ConcreteGrade": {
        "properties": {
          "design_code": {
            "type": "string",
            "title": "Design Code",
            "description": "Design code",
            "default": "ACI318M-19"
          },
          "grade": {
            "type": "string",
            "title": "Grade",
            "description": "Grade of the concrete",
            "default": "C12"
          }
        },
        "type": "object",
        "title": "GSD Concrete Grade",
        "dataclassname": "moapy.rc_pre.ConcreteGrade"
      },
      "Geometry": {
        "properties": {
          "concrete": {
            "description": "Concrete geometry",
            "default": {
              "material": {
                "design_code": "ACI318M-19",
                "grade": "C12"
              },
              "outerPolygon": [
                {
                  "x": 0.0,
                  "y": 0.0
                },
                {
                  "x": 400.0,
                  "y": 0.0
                },
                {
                  "x": 400.0,
                  "y": 600.0
                },
                {
                  "x": 0.0,
                  "y": 600.0
                }
              ],
              "innerPolygon": [
                {
                  "x": 80.0,
                  "y": 80.0
                },
                {
                  "x": 320.0,
                  "y": 80.0
                },
                {
                  "x": 320.0,
                  "y": 520.0
                },
                {
                  "x": 80.0,
                  "y": 520.0
                }
              ]
            },
            "$ref": "#/components/schemas/ConcreteGeometry"
          },
          "rebar": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/RebarGeometry-Output"
              },
              {
                "type": "null"
              }
            ],
            "description": "Rebar geometry",
            "default": {
              "prop": {
                "area": 287.0,
                "material": {
                  "design_code": "ACI318M-19",
                  "grade": "Grade 420"
                }
              },
              "points": [
                {
                  "x": 40.0,
                  "y": 40.0
                },
                {
                  "x": 360.0,
                  "y": 40.0
                },
                {
                  "x": 360.0,
                  "y": 560.0
                },
                {
                  "x": 40.0,
                  "y": 560.0
                }
              ]
            }
          },
          "tendon": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/TendonGeometry"
              },
              {
                "type": "null"
              }
            ],
            "description": "Tendon geometry",
            "default": {
              "prop": {
                "area": 287.0,
                "material": {
                  "design_code": "ACI318M-19",
                  "grade": "Grade 420"
                },
                "prestress": 0.0
              },
              "points": []
            }
          }
        },
        "type": "object",
        "title": "GSD Geometry",
        "dataclassname": "moapy.rc_pre.Geometry"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Point": {
        "properties": {
          "x": {
            "type": "number",
            "title": "X"
          },
          "y": {
            "type": "number",
            "title": "Y"
          }
        },
        "type": "object",
        "required": [
          "x",
          "y"
        ],
        "title": "Point",
        "dataclassname": "moapy.data_pre.Point"
      },
      "RebarGeometry-Input": {
        "properties": {
          "prop": {
            "description": "properties of the rebar",
            "default": {
              "area": 287.0,
              "material": {
                "design_code": "ACI318M-19",
                "grade": "Grade 420"
              }
            },
            "$ref": "#/components/schemas/RebarProp"
          },
          "points": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Points",
            "description": "Rebar Points",
            "default": [
              {
                "x": 40.0,
                "y": 40.0
              },
              {
                "x": 360.0,
                "y": 40.0
              },
              {
                "x": 360.0,
                "y": 560.0
              },
              {
                "x": 40.0,
                "y": 560.0
              }
            ]
          }
        },
        "type": "object",
