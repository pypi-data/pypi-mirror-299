{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/python-executor/"
    },
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/execute?functionId=moapy/wgsd/wgsd_sectionproperty/input_polygon": {
      "post": {
        "summary": "Input Polygon",
        "operationId": "input polygon",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/input_polygon"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OuterPolygon"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "OuterPolygon": {
        "properties": {
          "points": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Points",
            "description": "Outer Polygon",
            "default": [
              {
                "x": 0.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 600.0
              },
              {
                "x": 0.0,
                "y": 600.0
              }
            ]
          }
        },
        "type": "object",
        "title": "GSD Outer Polygon",
        "dataclassname": "moapy.data_pre.OuterPolygon"
      },
      "Point": {
        "properties": {
          "x": {
            "type": "number",
            "title": "X"
          },
          "y": {
            "type": "number",
            "title": "Y"
          }
        },
        "type": "object",
        "required": [
          "x",
          "y"
        ],
        "title": "Point",
        "dataclassname": "moapy.data_pre.Point"
      },
      "Points": {
        "properties": {
          "points": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Points",
            "description": "Points",
            "default": [
              {
                "x": 0.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 600.0
              },
              {
                "x": 0.0,
                "y": 600.0
              }
            ]
          }
        },
        "type": "object",
        "title": "GSD Points",
        "dataclassname": "moapy.data_pre.Points"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "input_polygon": {
        "properties": {
          "points": {
            "$ref": "#/components/schemas/Points"
          }
        },
        "type": "object",
        "required": [
          "points"
        ],
        "title": "input polygon"
      }
    }
  }
}