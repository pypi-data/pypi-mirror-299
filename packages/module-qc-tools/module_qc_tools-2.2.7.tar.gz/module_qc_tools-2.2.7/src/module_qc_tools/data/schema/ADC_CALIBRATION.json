{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for output json file",
    "description": "Raw data json",
    "type": "array",
    "items": {
    "type": "array",
    "items": {
    "type": "object",
    "properties": {
      "serialNumber": {
        "description": "Chip serial number",
        "type": "string"
      },
      "testType": {
        "description": "Test type",
        "type": "string",
        "pattern": "^ADC_CALIBRATION$"
      },
      "results": {
       "description": "Results of measurement",
       "type": "object",
       "properties": {
       "Measurements": {
          "description": "Measurement data",
          "type": "object",
          "required": [
            "Vmux8",
            "ADC_Vmux8",
            "DACs_input",
            "Vmux30",
	    "ADC_Vmux30"
          ]
       },
       "Metadata": {
          "description": "Meta data",
          "type": "object",
          "required": [
            "ChipID",
            "Name",
            "FirmwareVersion",
            "FirmwareIdentifier",
            "ChannelConfig",
            "SoftwareVersion",
            "ChipConfigs",
            "SoftwareType"
         ]
       }
     }
    }
  }
 }
 }
}
