{
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "title": "Schema for output json file",
   "description": "Raw data json",
   "type": "array",
   "items": {
   "type": "array",
   "items": {
   "type": "object",
   "properties": {
      "serialNumber": {
        "description": "Chip serial number",
        "type": "string"
      },
      "testType": {
        "description": "Test type",
        "type": "string",
        "pattern": "^LP_MODE$"
      },
      "results": {
       "description": "Results of measurement",
       "type": "object",
       "properties": {
      "Measurements": {
         "description": "Measurement data",
         "type": "object",
         "required": [
            "FailingPixels",
            "Temperature",
            "Current",
            "Vmux30",
            "Vmux33",
            "Vmux37",
            "Vmux36",
            "Imux0",
            "Imux28",
            "Imux29",
            "Imux30",
            "Imux31",
            "Imux63"
         ]
      },
      "Metadata": {
         "description": "Meta data",
         "type": "object",
         "required":[
           "ChipID",
           "Name",
           "FirmwareVersion",
           "FirmwareIdentifier",
           "ChannelConfig",
           "SoftwareVersion",
           "ChipConfigs",
           "SoftwareType",
           "AverageTemperature"
         ]
      }
      }
   }
  }
 }
 }
}
